<body>
<% include ./Partials/header.ejs %> 
<div class="container">
    <h1 class="brand"><span>GenFlow AI Pvt.Ltd</span></h1>
    <div class="wrapper animated bounceInLeft">
      <div class="company-info">
        
      </div>
      <div class="contact">
        <h3>Drop Your Information For Demo Request</h3>
        <form action="/cateringform" method="POST">
          <p>
            <label>Name</label>
            <input type="text" name="name" placeholder="Name">
          </p>
          <p>
            <label>Company Name</label>
            <input type="text" name="companyName" placeholder="Company Name">
          </p>
          <div class="form-group">
          <label for="exampleFormControlSelect1">Investor</label>
          <select class="form-control" id="exampleFormControlSelect1">
            <option>Company</option>
            <option>Individual</option>
            <option>Others</option>
          </select>
          </div>
          <p>
            <label>Address</label>
            <input type="text" name="address" placeholder="Address">
          </p>
          <p>
            <label>E-mail</label>
            <input type="email" name="email" placeholder="Email">
          </p>
          <p>
            <label>Contact</label>
            <input type="number" name="contact" placeholder="Contact">
          </p>
          <p class="full" rows="4" cols="50">
            <label >Comment</label>
            <textarea rows="4" cols="50" name="comment">Comment</textarea>
          </p>
          <p class="full">
            <button>Submit</button>
          </p>
        </form>
      </div>
    </div>
  </div>



